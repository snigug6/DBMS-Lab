1.
SELECT * FROM CLIENT_MASTER WHERE NAME LIKE '_a%';

2.
SELECT * FROM CLIENT_MASTER WHERE CITY LIKE '_a%';

3.
SELECT * FROM CLIENT_MASTER WHERE CITY IN ('Bombay','Delhi');

4.
SELECT * FROM CLIENT_MASTER WHERE BALANCE > 10000;

5.
SELECT * FROM SALES_ORDER WHERE TO_CHAR(ORDERDATE,'Mon') = 'Jan';

6.
SELECT * FROM SALES_ORDER WHERE CLIENTNO IN ('C00001','C00002');

7.
SELECT * FROM PRODUCT_MASTER WHERE SELLPRICE > 2000 AND SELLPRICE <= 5000;

8.
SELECT SELLPRICE, SELLPRICE * 0.15 "new_price" FROM PRODUCT_MASTER WHERE SELLPRICE > 1500;

9.
SELECT NAME, CITY, STATE FROM CLIENT_MASTER WHERE CITY NOT LIKE 'Maharashtra';

10.
SELECT COUNT(ORDERNO) FROM SALES_ORDER;

11.
SELECT AVG(SELLPRICE) FROM PRODUCT_MASTER;

12.
SELECT MAX(SELLPRICE) "max_price", MIN(SELLPRICE) "min_price" FROM PRODUCT_MASTER;

13.
SELECT COUNT(PRODUCTNO) FROM PRODUCT_MASTER WHERE SELLPRICE >= 1500;

14.
SELECT * FROM PRODUCT_MASTER WHERE QTYINHAND < REORDERLVL;

15.
SELECT ORDERNO, ORDERDATE FROM SALES_ORDER;
